@section Scripts {
    <script>
        $(".circlee-like").click(function () {
            console.log($(this).children()[0].classList.contains('fa-heart-o'));
            if ($(this).children()[0].classList.contains('fa-heart-o')) {
                $(this).children()[0].classList.remove('fa-heart-o');
                $(this).children()[0].classList.add('fa-heart');
            } else {
                $(this).children()[0].classList.remove('fa-heart');
                $(this).children()[0].classList.add('fa-heart-o');
            }
        });
    </script>
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


