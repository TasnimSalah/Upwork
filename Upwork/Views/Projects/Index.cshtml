@model IEnumerable<Upwork.Models.Project>

@{
    ViewData["Title"] = "Project List";
}
@section Style {
    <link href="~/css/IndexProject.css" rel="stylesheet" />
}
<div class="container">
    <div id="Project-list" class="wrapper">
        <div class="container">
            <!--Head-->
            <div class="row justify-content-between">
                <h2>Projects</h2>
                <a asp-action="create" class="devBtnGreen text-center px-3 ">
                    Create Project
                </a>
            </div>
            <!--end head-->
            <!--project tabs-->
            <div class="">
                <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Profile</a>
                    </li>

                </ul>
                <div class="tab-content" id="myTabContent">
                    <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                        <div class="project">
                            <div class="container">
                                <div class="row align-items-center">
                                    <div class="col-10">
                                        <div class="d-flex">
                                            <img src="~/Images/16938730_371690419878868_4123771690068118215_n.jpg" class="project-img" />
                                            <p class="project-title">You will get .net fullstack developer will deliver and how it benefits them</p>
                                        </div>


                                    </div>
                                    <div class="col-2">
                                        <div class=""
                                             style="width: 3rem; height: 3rem; margin: 0;padding:0;">
                                            <span class=" circlee-points container-circlee" data-toggle="dropdown"
                                                  aria-haspopup="true" aria-expanded="false">
                                                <i class="fa fa-ellipsis-h 2x" style="color:rgb(83, 83, 83) ;"></i>
                                            </span>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="#">Edit</a>
                                                <a class="dropdown-item" href="#">Remove</a>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>
                    <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                        <div class="project">
                            <div class="container">
                                <div class="row align-items-center">
                                    <div class="col-10">
                                        <div class="d-flex">
                                            <img src="~/Images/16938730_371690419878868_4123771690068118215_n.jpg" class="project-img" />
                                            <p class="project-title">You will get .net fullstack developer will deliver and how it benefits them</p>
                                        </div>


                                    </div>
                                    <div class="col-2">
                                        <div class=""
                                             style="width: 3rem; height: 3rem; margin: 0;padding:0;">
                                            <span class=" circlee-points container-circlee" data-toggle="dropdown"
                                                  aria-haspopup="true" aria-expanded="false">
                                                <i class="fa fa-ellipsis-h 2x" style="color:rgb(83, 83, 83) ;"></i>
                                            </span>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="#">Edit</a>
                                                <a class="dropdown-item" href="#">Remove</a>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>


                        </div>
                    </div>

                </div>
            </div>
            <div class="wrapper">
                <!--Head-->
                <div class="row border-bottom pb-4 pt-2">
                    <div class="container">
                        <h4>How Project Catalog works</h4>
                    </div>
                </div>
                <!--end head-->
                <div class="row">
                    <div class="col-md-3 col-12 my-4 ">
